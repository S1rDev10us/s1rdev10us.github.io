{
	"extends": "astro/tsconfigs/strictest",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		// "allowImportingTsExtensions": true
		"baseUrl": ".",
		"paths": {
			"@components/*": [
				"./src/components/*"
			],
			"@scripts/*": [
				"./src/scripts/*"
			],
			"@layouts/*": [
				"./src/layouts/*"
			]
		}
	},
	"exclude": [
		"old/*"
	]
}

---
import type classNames from "classnames";
export interface Props {
	src: string;
	style?: string;
	"class:list"?: classNames.Argument;
}
const { src, style } = Astro.props;
---

<script src="../scripts/components/PixelImage.ts"></script>
<canvas
	is="pixel-image"
	data-src={src}
	style={style}
	class:list={[Astro.props["class:list"]]}
></canvas>
